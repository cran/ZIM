\name{bshift}
\alias{bshift}
\title{Backshift Operator}
\usage{
bshift(x, k = 1)
}
\arguments{
  \item{x}{univariate or multivariate time series.}

  \item{k}{number of lags.}
}
\description{
Apply the backshift operator or lag operator to a time
series objective.
}
\examples{
x <- arima.sim(model = list(ar = 0.8, sd = 0.5), n = 120)
bshift(x, k = 12)
}
\seealso{
\code{\link[dplyr]{lag}}, \code{\link[TSA]{zlag}}
}
\keyword{misc}

